Context = createContext<IDressContext>({\r\n  dresses: [],\r\n  size: \"\",\r\n  sort: \"\",\r\n  email: \"\",\r\n  name: \"\",\r\n  address: \"\",\r\n  cartItems: [],\r\n  createOrderItems: (item: string) => {},\r\n  setCartItems: (cartItems: Dress[]) => {},\r\n  setProduct: (product: Dress) => {},\r\n  addToCart: (dress: Dress) => {},\r\n  sortDresses: (sort: any) => {},\r\n  filterDresses: (size: any) => {},\r\n  removeFromCart: (id: number) => {},\r\n});\r\n\r\nconst myDresses = () => {\r\n  const localData = localStorage.getItem('cartItems');\r\n  return localData ? JSON.parse(localData) : [];\r\n}\r\n\r\nconst DressContextPovider: React.FC = ({ children }) => {\r\n  const [dresses, setDresses] = useState<Dress[]>(products);\r\n  const [size, setSize] = useState<string>(\"\");\r\n  const [sort, setSort] = useState<string>(\"\");\r\n  const [cartItems, setCartItems] = useState<Dress[]>(myDresses);\r\n  const [email, setEmail] = useState<string>(\"\");\r\n  const [name, setName] = useState<string>(\"\");\r\n  const [address, setAddress] = useState<string>(\"\");\r\n  const [ product, setProduct ] = useState<Dress>()\r\n\r\n  useEffect(() => {\r\n    localStorage.setItem('cartItems', JSON.stringify(cartItems))\r\n  }, [cartItems]);\r\n\r\n  const removeFromCart = (id: number) => {\r\n    setCartItems(cartItems.filter(item => item.id !== id));\r\n  }\r\n\r\n  let createOrderItems = (item: any) => {\r\n    const order = {\r\n        name: name,\r\n        email: email,\r\n        address: address,\r\n        cartItems: cartItems,\r\n    };\r\n    createOrderItems(order);   \r\n  }\r\n\r\n  createOrderItems = (name: string) => {\r\n    alert(\"Need to save order for \" + name);\r\n  }\r\n\r\n  const addToCart = (product: Dress) => {\r\n    let alreadyInCart = false;\r\n    cartItems.forEach(item => {\r\n      if(item.id === product.id) {\r\n        item.count++;\r\n        alreadyInCart = true;\r\n      }\r\n    });\r\n    if(!alreadyInCart) {\r\n      cartItems.push({...product, count: 1})\r\n    }\r\n    setCartItems(cartItems);\r\n  }\r\n\r\n  const sortDresses = ({target: { value }, \r\n  }: ChangeEvent<HTMLSelectElement>) => {\r\n    // imp\r\n    if (value === \"\") {\r\n      setSort(value);\r\n    } else {\r\n      setSort(value);\r\n      setDresses(\r\n        products.slice().sort((a, b) => (\r\n          value === \"lowest\"\r\n          ? a.price > b.price\r\n            ? 1\r\n            : -1\r\n          : value === \"highest\"\r\n          ? a.price < b.price\r\n            ? 1\r\n            : -1\r\n          : a.id < b.id\r\n            ? 1\r\n            : -1\r\n        ))\r\n      );\r\n    }\r\n  };\r\n\r\n  const filterDresses = ({\r\n    target: { value },\r\n  }: ChangeEvent<HTMLSelectElement>) => {\r\n    // imp\r\n    if (value === \"\") {\r\n      setSize(value);\r\n    } else {\r\n      setSize(value);\r\n      setDresses(\r\n        products.filter((dress) =>\r\n          value === \"ALL\" ? true : dress.availableSizes.indexOf(value) >= 0\r\n        )\r\n      );\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div>\r\n      <DressContext.Provider\r\n        value={{ dresses, size, sort, email, name, address, setProduct, cartItems, removeFromCart, createOrderItems, addToCart, setCartItems, sortDresses, filterDresses }}\r\n      >\r\n        {children}\r\n      </DressContext.Provider>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default DressContextPovider;\r\n","C:\\Users\\lubwi\\OneDrive\\Skrivbord\\REACT\\TYPESCRIPT\\syl-shopping-cart\\src\\index.tsx",[],["44","45"],"C:\\Users\\lubwi\\OneDrive\\Skrivbord\\REACT\\TYPESCRIPT\\syl-shopping-cart\\src\\util.ts",[],"C:\\Users\\lubwi\\OneDrive\\Skrivbord\\REACT\\TYPESCRIPT\\syl-shopping-cart\\src\\data.ts",[],"C:\\Users\\lubwi\\OneDrive\\Skrivbord\\REACT\\TYPESCRIPT\\syl-shopping-cart\\src\\App.tsx",[],"C:\\Users\\lubwi\\OneDrive\\Skrivbord\\REACT\\TYPESCRIPT\\syl-shopping-cart\\src\\components\\Filter.tsx",[],{"ruleId":"46","severity":1,"message":"47","line":5